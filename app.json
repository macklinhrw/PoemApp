{
  "expo": {
    "name": "PoemApp",
    "slug": "PoemApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android-icon.png",
    "splash": {
      "image": "./assets/android-chrome-384x384.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-chrome-384x384.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
